{
  "name": "mobile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "expo": "^48.0.0",
    "react": "18.2.0",
    "react-native": "0.71.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.7.2",
    "@react-native-community/slider": "^4.4.2"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.70.6",
    "jest": "^29.3.1",
    "jest-expo": "^48.0.0",
    "@testing-library/react-native": "^11.0.0",
    "ts-jest": "^29.0.3",
    "@types/jest": "^29.2.3",
    "@testing-library/jest-native": "^5.0.0",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest.rn.setup.stub.js"
    ],
    "moduleNameMapper": {
      "^@react-native/polyfills/error-guard$": "<rootDir>/jest.polyfills.stub.js",
      "^react-native/jest/setup$": "<rootDir>/jest.rn.setup.stub.js",
      "^react-test-renderer$": "<rootDir>/../../node_modules/.pnpm/react-test-renderer@18.2.0_react@18.2.0/node_modules/react-test-renderer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?(jest-)?react-native|(?:.pnpm/)?@react-native|(?:.pnpm/)?react-native-.*|(?:.pnpm/)?@react-navigation|(?:.pnpm/)?@react-native-community|(?:.pnpm/)?expo(nent)?|(?:.pnpm/)?@expo(nent)?/.*|(?:.pnpm/)?expo-modules-core|(?:.pnpm/)?react-native-svg|(?:.pnpm/)?@unimodules/.*|(?:.pnpm/)?@react-native/polyfills)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
}